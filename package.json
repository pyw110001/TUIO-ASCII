{
  "name": "tuio-ascii-converter",
  "version": "1.0.0",
  "description": "TUIO to Binary TCP Protocol Converter",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
