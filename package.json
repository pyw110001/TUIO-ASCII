    "start": "node server/index.js"
  },
  "keywords": ["tuio", "osc", "tcp", "converter"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "osc-js": "^2.4.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}

